{
  "name": "vscode-plugin-removelog",
  "displayName": "Remove Log - 移除日志语句",
  "description": "快速移除代码中的 console.log、console.error 等日志语句",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-plugin-removelog.removeLogs",
        "title": "移除日志语句",
        "category": "Remove Log"
      },
      {
        "command": "vscode-plugin-removelog.removeLogsInFile",
        "title": "移除当前文件中的日志语句",
        "category": "Remove Log"
      },
      {
        "command": "vscode-plugin-removelog.removeLogsInWorkspace",
        "title": "移除工作区中的日志语句",
        "category": "Remove Log"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-plugin-removelog.removeLogsInFile",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
